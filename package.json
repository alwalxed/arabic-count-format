{
  "name": "arabic-count-format",
  "version": "1.0.2",
  "description": "A minimal, zero-dependency utility for pluralizing Arabic nouns across singular, dual, and plural forms, including 0 and 11+ counts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "arabic",
    "pluralization",
    "grammar",
    "nouns",
    "singular",
    "dual",
    "plural",
    "number-formatting",
    "inflection",
    "i18n",
    "internationalization",
    "localization",
    "arabic-grammar",
    "count-formatting"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "author": "Awa <alwalxed@proton.me>",
  "license": "MIT",
  "homepage": "https://github.com/alwalxed/arabic-count-format",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alwalxed/arabic-count-format"
  },
  "bugs": {
    "url": "https://github.com/alwalxed/arabic-count-format/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/alwalxed"
  }
}
